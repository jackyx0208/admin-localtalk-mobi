* @api public\n */\n\n\nBackoff.prototype.duration = function () {\n  var ms = this.ms * Math.pow(this.factor, this.attempts++);\n\n  if (this.jitter) {\n    var rand = Math.random();\n    var deviation = Math.floor(rand * this.jitter * ms);\n    ms = (Math.floor(rand * 10) & 1) == 0 ? ms - deviation : ms + deviation;\n  }\n\n  return Math.min(ms, this.max) | 0;\n};\n/**\n * Reset the number of attempts.\n *\n * @api public\n */\n\n\nBackoff.prototype.reset = function () {\n  this.attempts = 0;\n};\n/**\n * Set the minimum duration\n *\n * @api public\n */\n\n\nBackoff.prototype.setMin = function (min) {\n  this.ms = min;\n};\n/**\n * Set the maximum duration\n *\n * @api public\n */\n\n\nBackoff.prototype.setMax = function (max) {\n  this.max = max;\n};\n/**\n * Set the jitter\n *\n * @api public\n */\n\n\nBackoff.prototype.setJitter = function (jitter) {\n  this.jitter = jitter;\n};\n\n/***/ })\n\n}]);","inputSourceMap":{"version":3,"sources":["E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/engine.io-parser/lib/browser.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/socket.io-client/node_modules/debug/src/browser.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/parseqs/index.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/component-inherit/index.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/engine.io-client/node_modules/debug/src/browser.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/socket.io-parser/index.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/engine.io-client/lib/xmlhttprequest.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/engine.io-client/lib/transport.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/engine.io-client/node_modules/component-emitter/index.js","E:/projects/localtalk.mobi/mobilechat-adminpanel/node_modules/parseuri/index.js","E:/projects/localtalk.mobi/mobilechat-adminpane